from app import db
from app.models import User

# Update all users who don't have a profile_image set
users_without_images = User.query.filter_by(profile_image=None).all()
for user in users_without_images:
    user.profile_image = 'image/default_profile.jpg'

db.session.commit()
print(f"Updated {len(users_without_images)} users with default profile images.")
